{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "outDir": "dist",
        "sourceMap": false,
        "strict": true,
        "resolveJsonModule": true,
        "rootDir": "src",
        "strictPropertyInitialization": false,
        "noImplicitReturns": false,

    },
    "compileOnSave": true,
    "include": [
        "src/**/*.ts", "src/**/*.js", "src/**/*.json", 
    ]    
}